<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kennethallan.Bloom_TB_002.Summary">

    <FrameLayout
        android:id="@+id/Fragment_Holder"
        android:layout_width="383dp"
        android:layout_height="370dp">

    </FrameLayout>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/lv_SessionActivities"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/Fragment_Holder"
        android:layout_margin="5dp" />

    <Button
        android:text="Carry Over?"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/bn_CarryOver"
        android:layout_below="@+id/lv_SessionActivities"
        android:layout_centerHorizontal="true"/>

</RelativeLayout>
